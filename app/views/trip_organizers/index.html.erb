<p id="notice"><%= notice %></p>

<h1>Trip Organizers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trip_organizers.each do |trip_organizer| %>
      <tr>
        <td><%= link_to 'Show', trip_organizer %></td>
        <td><%= link_to 'Edit', edit_trip_organizer_path(trip_organizer) %></td>
        <td><%= link_to 'Destroy', trip_organizer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trip Organizer', new_trip_organizer_path %>
